    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ВЫБОР
		КОГДА ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Приемка)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.ПриемкаКакЕсть)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.КонтрольнаяАгрегация)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Инвентаризация)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Отгрузка)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Проверка)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Наборка)
			ТОГДА ИтЗадачиНаТСД.ДокументОснование
		ИНАЧЕ ИтЗадачиНаТСД.ИдентификаторУпаковки
	КОНЕЦ КАК Задача,
	ИтЗадачиНаТСД.ТипЗадачи КАК ТипЗадачи,
	ВЫБОР
		КОГДА ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Приемка)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.ПриемкаКакЕсть)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.КонтрольнаяАгрегация)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Инвентаризация)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Отгрузка)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Проверка)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Наборка)
			ТОГДА &quot;&quot;
		ИНАЧЕ ИтЗадачиНаТСД.ЯчейкаОтправительПредставление
	КОНЕЦ КАК ЯчейкаОтправитель,
	ВЫБОР
		КОГДА ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Приемка)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.ПриемкаКакЕсть)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.КонтрольнаяАгрегация)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Инвентаризация)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Отгрузка)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Проверка)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Наборка)
			ТОГДА &quot;&quot;
		ИНАЧЕ ИтЗадачиНаТСД.ЯчейкаПолучательПредставление
	КОНЕЦ КАК ЯчейкаПолучатель,
	ВЫБОР
		КОГДА ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.Приемка)
				ИЛИ ИтЗадачиНаТСД.ТипЗадачи = ЗНАЧЕНИЕ(перечисление.итWMSТипыЗадачТСД.ПриемкаКакЕсть)
